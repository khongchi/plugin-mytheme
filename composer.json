{
    "name": "xpressengine-plugin/mytheme",
    "description": "이 플러그인은 테마 만들기 실습을 위해 제작된  플러그인입니다. XE Basic 템플릿을 기반으로 하는 테마입니다",
    "keywords": [
        "xpressengine",
        "plugin",
		"xe basic"
    ],
    "license": "LGPL-2.0",
    "version": "1.0.0",
    "type": "xpressengine-plugin",
    "support": {
        "email": "khongchi@xpressengine.com"
    },
    "authors": [
        {
            "name": "khongchi",
            "email": "khongchi@xpressengine.com",
            "homepage": "http://github.com/khongchi",
            "role": "Developer"
        }
    ],
    "extra": {
        "xpressengine": {
            "title": "my theme",
            "component": {
                "theme/mytheme@basic": {
                    "class": "Khongchi\\Plugins\\MyTheme\\Theme\\Basic",
                    "name": "BasicTheme",
                    "description": "Basic Theme supported by Mytheme plugin."
                },
                "theme/mytheme@main": {
                    "class": "Khongchi\\Plugins\\MyTheme\\Theme\\Main",
                    "name": "MainTheme",
                    "description": "Main page Theme supported by Mytheme plugin."
                }
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.xpressengine.io/"
        }
    ],
    "require": [],
    "autoload": {
        "psr-4": {
            "Khongchi\\Plugins\\MyTheme\\": "src/"
        }
    }
}
